/*the following program is the pattern type output is below
*	*	*	*	*	
	*	*	*	*	
		*	*	*	
			*	*	
				*	
*/

#include <stdio.h>

int main()
{
    int i=0,j=0,n=5;
    for(i=1;i<=n;i++){
        for(j=1;j<=n;j++){
            if(j<i){
                printf("\t");
            }else{
            printf("*\t");
            }
        }printf("\n");
    }
    return 0;
}
